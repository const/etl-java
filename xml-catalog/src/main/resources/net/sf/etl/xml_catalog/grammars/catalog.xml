<!DOCTYPE catalog PUBLIC "-//OASIS//DTD XML Catalogs V1.1//EN" "./oasis_xml_catalogs_1_1/catalog.dtd">
<catalog xmlns:rddl="http://www.rddl.org/" xmlns="urn:oasis:names:tc:entity:xmlns:xml:catalog" prefer="public">
    <!-- This is a bootstrap catalog that is used to resolve catalog grammars -->
    <group xml:base="oasis_xml_catalogs_1_0/">
        <public publicId="-//OASIS//DTD Entity Resolution XML Catalog V1.0//EN" uri="catalog.dtd"/>

        <rewriteSystem systemIdStartString="http://www.oasis-open.org/committees/entity/release/1.0/" rewritePrefix=""/>
    </group>

    <group xml:base="oasis_xml_catalogs_1_1/">
        <public publicId="-//OASIS//DTD XML Catalogs V1.1//EN" uri="catalog.dtd"/>

        <rewriteSystem systemIdStartString="http://www.oasis-open.org/committees/entity/release/1.1/" rewritePrefix=""/>

        <uri name="urn:oasis:names:tc:entity:xmlns:xml:catalog"
             rddl:nature="http://www.w3.org/2001/XMLSchema"
             rddl:purpose="http://www.rddl.org/purposes#schema-validation"
             uri="catalog.xsd"/>

        <uri name="urn:oasis:names:tc:entity:xmlns:xml:catalog"
             rddl:nature="http://relaxng.org/ns/structure/1.0"
             rddl:purpose="http://www.rddl.org/purposes#schema-validation"
             uri="catalog.rng"/>

        <uri name="urn:oasis:names:tc:entity:xmlns:xml:catalog"
             rddl:nature="http://www.isi.edu/in-notes/iana/assignments/media-types/application/xml-dtd"
             rddl:purpose="http://www.rddl.org/purposes#validation"
             uri="catalog.dtd"/>
    </group>
</catalog>